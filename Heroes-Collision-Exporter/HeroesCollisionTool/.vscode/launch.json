   "version": "0.2.0",
   "configurations": [
       
        {
            "name": "[Release] .NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build Release",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/HeroesCollisionTool/bin/Release/netcoreapp2.0/HeroesCollisionTool.dll",
            "args": 
            [ 
                "--file", 
                "D:/Desktop/Github/HeroesCollisionLibrary/Heroes-Collision-Exporter/HeroesCollisionTool/HeroesCollisionTool/bin/Debug/netcoreapp2.0/s01.cl"
            ],
            "cwd": "${workspaceFolder}/HeroesCollisionTool",
            // For more information about the 'console' field, see https://github.com/OmniSharp/omnisharp-vscode/blob/master/debugger-launchjson.md#console-terminal-window
            "console": "integratedTerminal",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": "[Debug] .NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/HeroesCollisionTool/bin/Debug/netcoreapp2.0/HeroesCollisionTool.dll",
            "args": 
            [ 
                "--file", 
                "D:/Desktop/Github/HeroesCollisionLibrary/Heroes-Collision-Exporter/HeroesCollisionTool/HeroesCollisionTool/bin/Debug/netcoreapp2.0/s01.cl"
            ],
            "cwd": "${workspaceFolder}/HeroesCollisionTool",
            // For more information about the 'console' field, see https://github.com/OmniSharp/omnisharp-vscode/blob/master/debugger-launchjson.md#console-terminal-window
            "console": "integratedTerminal",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}